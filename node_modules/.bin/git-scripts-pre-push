#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@chatie/git-scripts/dist/bin/pre-push.js" "$@"
else 
  exec node  "$basedir/../@chatie/git-scripts/dist/bin/pre-push.js" "$@"
fi
